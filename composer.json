{
    "name": "horde/content",
    "description": "Tagging application",
    "type": "horde-application",
    "homepage": "https://www.horde.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Michael J Rubinsky",
            "email": "mrubinsk@horde.org",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        }
    ],
    "version": "3.0.0alpha1",
    "time": "2021-02-24",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "*",
        "php": "^5.3 || ^7",
        "horde/core": "^2",
        "horde/date": "^2",
        "horde/exception": "^2",
        "horde/db": "^2",
        "horde/injector": "^2",
        "horde/rdo": "^2",
        "horde/util": "^2",
        "ext-gettext": "*",
        "ext-json": "*"
    },
    "suggest": {
        "horde/argv": "^2",
        "horde/controller": "^2",
        "horde/elasticsearch": "^1"
    },
    "autoload": {
        "psr-0": {
            "Content": "lib/"
        },
        "psr-4": {
            "Horde\\Content\\": "src/"
        }
    },
    "bin": [
        "bin/content-object-add",
        "bin/content-object-delete",
        "bin/content-tag",
        "bin/content-tag-add",
        "bin/content-tag-delete",
        "bin/content-untag"
    ]
}